<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thithu">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ThiThu">
        <activity
            android:name=".Sua_kh"
            android:label="@string/title_activity_sua_kh"
            android:theme="@style/Theme.ThiThu.NoActionBar" />
        <activity
            android:name=".Them_kh"
            android:label="@string/title_activity_them_kh"
            android:theme="@style/Theme.ThiThu.NoActionBar" />
        <activity
            android:name=".trangchu"
            android:label="@string/title_activity_trangchu"
            android:theme="@style/Theme.ThiThu.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.ThiThu.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>